{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "about.html",
      "use": "@vercel/static"
    },
    {
      "src": "services.html",
      "use": "@vercel/static"
    },
    {
      "src": "projects.html",
      "use": "@vercel/static"
    },
    {
      "src": "contact.html",
      "use": "@vercel/static"
    },
    {
      "src": "certifications.html",
      "use": "@vercel/static"
    },
    {
      "src": "admin.html",
      "use": "@vercel/static"
    },
    {
      "src": "maintenance.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/about", "dest": "/about.html" },
    { "src": "/services", "dest": "/services.html" },
    { "src": "/projects", "dest": "/projects.html" },
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/certifications", "dest": "/certifications.html" },
    { "src": "/admin", "dest": "/admin.html" },
    { "src": "/admin-backend", "dest": "/admin-backend.html" },
    { "src": "/maintenance", "dest": "/maintenance.html" }
  ]
}
